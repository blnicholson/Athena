{
  "name": "athena",
  "version": "1.0.0",
  "description": "MERN Stack User Authentication",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "elemental": "^0.6.1",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongoose": "^5.3.9",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "query-string": "^6.2.0",
    "randomstring": "^1.1.5",
    "react-big-calendar": "^0.20.2",
    "react-dropzone": "^7.0.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.5"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Cody Thompson, Brandy Nicholson, Dustin McGilvray",
  "license": "ISC"
}
